<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/tab_02"
    android:orientation="vertical"
    >

    <TextView
        android:text="终止时间："
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        android:layout_alignBaseline="@+id/end_time"
        android:layout_alignBottom="@+id/end_time"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/begin_time"
        android:hint="如           07-00"
        android:layout_alignBaseline="@+id/textView6"
        android:layout_alignBottom="@+id/textView6"
        android:layout_toEndOf="@+id/textView6" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/end_time"
        android:hint="如         19-00"
        android:layout_below="@+id/begin_time"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/textView7" />

    <TextView
        android:text="起始时间："
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:layout_marginTop="16dp"
        android:layout_below="@+id/textView3"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="请设置第一段时间的收费（注意两段时间之和必须为24小时，时间格式为24小时制，请准确输入以免造成错误计算）"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView3"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/bt_twotime"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="小型车："
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView15"
        android:layout_alignBaseline="@+id/littercar1"
        android:layout_alignBottom="@+id/littercar1"
        android:layout_alignParentStart="true" />


    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/littercar1"
        android:hint="请输入数字"
        android:layout_below="@+id/end_time"
        android:layout_toEndOf="@+id/textView15" />

    <TextView
        android:text="元/小时"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView15"
        android:layout_toEndOf="@+id/littercar1"
        android:layout_marginStart="12dp"
        android:id="@+id/textView17" />

    <TextView
        android:text="请设置第二段时间的收费（注意两段时间之和必须为24小时，时间格式为24小时制，请准确输入以免造成错误计算）"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView23"
        android:layout_below="@+id/bigcar1"
        android:layout_marginTop="40dp"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="小型车："
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView23"
        android:layout_alignParentStart="true"
        android:layout_marginTop="20dp"
        android:id="@+id/textView24" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/bigcar2"
        android:hint="请输入数字"
        android:layout_alignBaseline="@+id/textView24"
        android:layout_alignBottom="@+id/textView24"
        android:layout_toEndOf="@+id/textView24" />

    <TextView
        android:text="元/小时"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView26"
        android:layout_alignBaseline="@+id/bigcar2"
        android:layout_alignBottom="@+id/bigcar2"
        android:layout_alignStart="@+id/textView21" />

    <TextView
        android:text="大型车："
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView20"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/textView21"
        android:layout_alignBottom="@+id/textView21" />

    <TextView
        android:text="元/小时"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView21"
        android:layout_below="@+id/littercar1"
        android:layout_alignStart="@+id/textView17"
        android:layout_marginTop="33dp" />

    <Button
        android:text="确认"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bt_twotime"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="元/小时"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView27"
        android:layout_alignBaseline="@+id/littercar2"
        android:layout_alignBottom="@+id/littercar2"
        android:layout_alignStart="@+id/textView26" />

    <TextView
        android:text="大型车："
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView25"
        android:layout_alignBaseline="@+id/littercar2"
        android:layout_alignBottom="@+id/littercar2"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/littercar2"
        android:hint="请输入数字"
        android:layout_marginTop="16dp"
        android:layout_below="@+id/bigcar2"
        android:layout_toEndOf="@+id/textView25" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/bigcar1"
        android:hint="请输入数字"
        android:layout_alignBaseline="@+id/textView20"
        android:layout_alignBottom="@+id/textView20"
        android:layout_toEndOf="@+id/textView20" />


</RelativeLayout>
